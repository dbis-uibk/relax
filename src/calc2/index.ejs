		.spinnerInit {
			background-color: #333;
			width: 64px;
			height: 64px;
			border-radius: 100%;
			-webkit-animation: sk-scaleout-init 1.0s infinite ease-in-out;
			animation: sk-scaleout-init 1.0s infinite ease-in-out;
		}

		@-webkit-keyframes sk-scaleout-init {
			0% {
				-webkit-transform: scale(0)
			}

			100% {
				-webkit-transform: scale(1.0);
				opacity: 0;
			}
		}

		@keyframes sk-scaleout-init {
			0% {
				-webkit-transform: scale(0);
				transform: scale(0);
			}

			100% {
				-webkit-transform: scale(1.0);
				transform: scale(1.0);
				opacity: 0;
			}
		}
